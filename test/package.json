{
  "name": "bedrock-meter-usage-reporter-test",
  "version": "0.0.1-0",
  "type": "module",
  "private": true,
  "scripts": {
    "test": "node --preserve-symlinks test.js test",
    "test:debug": "node --preserve-symlinks test.js test --log-level debug",
    "coverage": "cross-env NODE_ENV=test c8 --reporter=lcov --reporter=text-summary npm test",
    "coverage-ci": "cross-env NODE_ENV=test c8 --reporter=lcovonly npm test",
    "coverage-report": "c8 report"
  },
  "dependencies": {
    "@bedrock/app-identity": "^3.0.0",
    "@bedrock/core": "^6.0.0",
    "@bedrock/did-io": "^8.0.0",
    "@bedrock/express": "^8.0.0",
    "@bedrock/https-agent": "^4.0.0",
    "@bedrock/jsonld-document-loader": "^3.0.0",
    "@bedrock/meter": "^3.0.0",
    "@bedrock/meter-http": "^8.0.0",
    "@bedrock/meter-usage-reporter": "file:..",
    "@bedrock/mongodb": "^10.0.0",
    "@bedrock/server": "^5.0.0",
    "@bedrock/test": "^8.0.0",
    "@bedrock/zcap-storage": "^7.0.0",
    "@digitalbazaar/ed25519-signature-2020": "^3.0.0",
    "@digitalbazaar/ezcap": "^2.0.2",
    "abort-controller": "^3.0.0",
    "c8": "^7.11.0",
    "cross-env": "^7.0.2",
    "sinon": "^9.2.4"
  },
  "c8": {
    "excludeNodeModules": false,
    "include": [
      "node_modules/@bedrock/meter-usage-reporter/**"
    ],
    "exclude": [
      "node_modules/@bedrock/meter-usage-reporter/node_modules/**"
    ]
  }
}
