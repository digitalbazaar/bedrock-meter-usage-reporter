{
  "name": "bedrock-meter-usage-reporter-test",
  "version": "0.0.1-0",
  "private": true,
  "scripts": {
    "test": "node --preserve-symlinks test.js test",
    "test:debug": "node --preserve-symlinks test.js test --log-level debugs",
    "coverage": "cross-env NODE_ENV=test ESM_OPTIONS='{cache:false}' nyc --reporter=lcov --reporter=text-summary npm test",
    "coverage-ci": "cross-env NODE_ENV=test ESM_OPTIONS='{cache:false}' nyc --reporter=lcovonly npm test",
    "coverage-report": "nyc report"
  },
  "dependencies": {
    "base58-universal": "^1.0.0",
    "bedrock": "^4.3.0",
    "bedrock-app-identity": "^1.0.0",
    "bedrock-did-context": "^2.0.2",
    "bedrock-did-io": "digitalbazaar/bedrock-did-io#main",
    "bedrock-edv-storage": "digitalbazaar/bedrock-edv-storage#zcapld-update",
    "bedrock-express": "^6.1.0",
    "bedrock-https-agent": "^2.0.0",
    "bedrock-jsonld-document-loader": "^1.2.0",
    "bedrock-kms": "^7.3.0",
    "bedrock-kms-http": "digitalbazaar/bedrock-kms-http#zcapld-update",
    "bedrock-ledger-context": "^20.0.1",
    "bedrock-meter": "^1.2.0",
    "bedrock-meter-http": "digitalbazaar/bedrock-meter-http#zcapld-update",
    "bedrock-meter-usage-reporter": "file:..",
    "bedrock-mongodb": "^8.0.1",
    "bedrock-package-manager": "^1.1.0",
    "bedrock-security-context": "^4.1.0",
    "bedrock-server": "^3.0.0",
    "bedrock-test": "^6.0.0",
    "bedrock-validation": "^5.2.2",
    "bedrock-veres-one-context": "^11.0.0",
    "bedrock-zcap-storage": "^4.1.1",
    "cross-env": "^7.0.2",
    "nyc": "^15.0.1",
    "sinon": "^9.0.2"
  },
  "nyc": {
    "excludeNodeModules": false,
    "include": [
      "node_modules/bedrock-meter-usage-reporter/**"
    ],
    "exclude": [
      "node_modules/bedrock-meter-usage-reporter/node_modules/**"
    ]
  }
}
